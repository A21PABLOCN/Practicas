;
; BIND data file for local loopback interface
;
$TTL	120
@	IN	SOA	aaylasecura.galaxia.lan. root.galaxia.lan. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 120 )	; Negative Cache TTL
;
@				IN	NS	aaylasecura.galaxia.lan.
aaylasecura		IN	A	192.168.20.239
yarua			IN	A	192.168.20.238
